[
- @hierarchy_entries.each_with_index do |he, i|
  - if i > 1
    = ",\n"
  {attributes: {
  ="id: '#{he.id}', "
  ="taxon_concept: '#{he.clean_taxon_concept}'"
  },
  - if (he.siblings_count > 0)
    = "state: 'closed',"
  - else
    = "state: 'leaf',"
  ="data: '#{he.clean_taxon_concept}', "
  ="taxon_concept_id: '#{he.taxon_concept_id}'"
  }
  = "," unless he == @hierarchy_entries.last
]
